{
  "comments": {
    "blockComment": ["<%!--", "--%>"]
  },
  "brackets": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["<", ">"],
    ["<%", "%>"]
  ],
  "autoClosingPairs": [
    { "open": "{", "close": "}" },
    { "open": "[", "close": "]" },
    { "open": "(", "close": ")" },
    { "open": "'", "close": "'", "notIn": ["string", "comment"] },
    { "open": "\"", "close": "\"", "notIn": ["string"] },
    { "open": "<%", "close": "%>" },
    { "open": "<%=", "close": "%>" },
    { "open": "<%!--", "close": "--%>" }
  ],
  "surroundingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["'", "'"],
    ["\"", "\""],
    ["<%", "%>"],
    ["<%=", "%>"]
  ],
  "folding": {
    "markers": {
      "start": "^\\s*<%",
      "end": "^\\s*%>"
    }
  }
}
